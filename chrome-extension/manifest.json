{
  "manifest_version": 3,
  "name": "MyMind Bookmark Saver",
  "version": "1.0.0",
  "description": "Save bookmarks instantly to your MyMind account",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}